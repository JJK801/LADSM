<!DOCTYPE html>
<html {{ sonata_seo_html_attributes() }}>
    <head {{ sonata_seo_head_attributes() }}>
        {{ sonata_seo_title() }}

        <!-- Meta -->

        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        {{ sonata_seo_metadatas() }}
        {{ sonata_seo_link_canonical() }}
        {{ sonata_seo_lang_alternates() }}

        <!-- CMF Style -->

        {% include "CmfCreateBundle::includecssfiles.html.twig" %}

        <!-- Custom Style -->

{% block stylesheets %}
{% stylesheets '%kernel.root_dir%/Resources/public/**/*.css' '%kernel.root_dir%/Resources/public/css/**/*.css' filter='yui_css' output='css/style.css' %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
{% endstylesheets %}
{% endblock %}
    </head>
    <body>
        <div id="wrapper">
            {% include "::header.html.twig" %}
            {% include "::breadcrumbs.html.twig" %}
            <div id="highlighted" class="container">
            {% block highlighted %}{% endblock %}
            </div>
            <div id="content" class="container">
                {% block content %}{% endblock %}
            </div>
            {% include "::footer.html.twig" %}
        </div>
{% block javascripts %}

        <script type="text/javascript" src="{{ asset("js/jquery-2.1.3.min.js") }}"></script>
        <script type="text/javascript" src="{{ asset("js/jquery.jcarousel.min.js") }}"></script>

        <!-- CMF Scripts -->

        {% render controller("cmf_create.jsloader.controller:includeJSFilesAction", {'_locale': app.request.locale}) %}

        <!-- Custom Script -->

{% javascripts '%kernel.root_dir%/Resources/public/**/*.js' '%kernel.root_dir%/Resources/public/js/**/*.js' filter='yui_css' output='js/script.js' %}
        <script src="{{ asset_url }}" type="text/javascript" ></script>
{% endjavascripts %}
{% endblock %}
    </body>
</html>